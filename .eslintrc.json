{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"semi": "error",
		"indent": ["error", 2],
		"no-var": "error",
		"camelcase": "error",
		"quotes": [2, "single", { "avoidEscape": true }],
		"react/no-unescaped-entities": 0
	}
}
